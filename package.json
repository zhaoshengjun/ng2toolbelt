{
    "name": "ng2toolbelt",
    "displayName": "ng2toolbelt",
    "description": "The missing toolbelt for Angular 2",
    "version": "0.0.1",
    "publisher": "samzhao",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.ng2toolbelt"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.ng2toolbelt",
            "title": "Ng2 Toolbelt"
        }]
    },
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^1.8.5",
        "vscode": "^0.11.0"
    }
}